<idea-plugin>
  <id>mr.intellij.plugin.autofactory</id>
  <name>Google Autofactory Support</name>
  <version>0.0.1</version>
  <vendor email="matan129@gmail.com">Matan Rosenberg</vendor>

  <description>Provides support for Google's AutoFactory.</description>

  <change-notes/>
  <idea-version since-build="145.0"/>
  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
  </extensions>
  <actions>
      <action id="mr.intellij.plugin.autofactory.actions.AutoFactoryOnClassAction"
              text="Add @AutoFactory to Class"
              class="mr.intellij.plugin.autofactory.actions.AutoFactoryOnClassAction"
              description="Adds @AutoFactory to a class"/>
  </actions>
</idea-plugin>
